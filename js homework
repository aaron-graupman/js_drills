4.
var vowelCount = function(str) {
  var count = 0;
  for(var i = 0; i < str.length; i++) {
    if(str[i] == 'a' || str[i] == 'i' || str[i] == 'o' ||str[i] == 'e' ||str[i] == 'u') {
      count+=1;
    }
  }
 console.log(count);
}
vowelCount('cupcake')


