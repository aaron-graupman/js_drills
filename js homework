var num1 = [1, 2, 3, 4, 5, 6]
var num2 = [1, 2, 3, 4, 5, 6]
var rand1 = num1[Math.floor(Math.random() * num1.length)];
var rand2 = num2[Math.floor(Math.random() * num2.length)];

var sum = rand1 + rand2
console.log(sum)


